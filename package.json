{
	"name": "squif",
	"displayName": "Squif",
	"description": "An Arma 3 Intellisense addon that pulls docs from the documenation portal: https://squif.co.za/docs \n\nRead more here: https://squif.co.za/",
	"version": "0.0.3",
	"icon": "fileicons/squif.png",
	"repository": {
		"url": "https://github.com/Dark-Coffee-Software/squif",
		"type": "git"
	},
	"publisher": "DarkCoffeeSoftware",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:squif.checker"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "squif.checker",
				"title": "Ini Squif",
				"category": "Run"
			}
		],
		"iconThemes": [
			{
				"id": "sqf",
				"label": "SQF",
				"path": "./fileicons/sqf-icon-theme.json"
			}
		],
		"languages": [
			{
				"id": "sqf",
				"extensions": [
					".sqf"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.55.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^10.1.0",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"axios": "^0.25.0",
		"fs": "0.0.1-security",
		"path": "^0.12.7",
		"regex": "^0.1.1"
	}
}
